func f():Int64 {
    var x = 1;
    try {
        return x + x; // Return 2.
    } catch (e: RuntimeException) { // Caught.
        print("RuntimeException and its subtypes can be caught here")
    } finally {
        x = 2;
    }
    return 0
}
func f1():Int64 {
    var x = 1;
    try {
        return x + x; // Return 2.
    } catch (e: RuntimeException) { // Caught.
        print("RuntimeException and its subtypes can be caught here")
    } finally {
        x = 2;
        return x + x; // Return 4
    } // The return value is 4 but not 2.
    return 0
}
